#include <iostream>
#include <stack>
#include <string>

using namespace std;

int main() {
    // Create a stack to store FAQs
    stack<string> faqStack;

    // Push FAQs into the stack
    faqStack.push("How to delete an event?");
    faqStack.push("How to edit an event?");
    faqStack.push("How to add an event?");
    faqStack.push("How to view all upcoming events?");
    faqStack.push("How to share an event with others?");

    // Display the FAQs in Last In, First Out (LIFO) order
    cout << "Frequently Asked Questions (FAQs):" << endl;
    while (!faqStack.empty()) {
        cout << "- " << faqStack.top() << endl;  // Display the question
        faqStack.pop();                         // Remove the question
    }

    return 0;
}
